		{
			"name": "Login",
			"item": [
				{
					"name": "login_luke",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "2a9bad17-545f-44e9-838a-ee97e8d09569",
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"var jsonData = pm.response.json();",
									"let keysInResponse = JSON.parse(pm.variables.get(\"loginResposeKey\"));",
									"",
									"pm.test(\"Response should not be 'null'\", function () {",
									"    pm.expect(jsonData).not.eql(null); ",
									"});",
									"",
									"pm.test(\"Response should not be 'empty object'\", function () {",
									"    pm.expect(jsonData).not.eql({}); ",
									"});",
									"",
									"pm.test(\"Response should not be 'empty array'\", function () {",
									"    pm.expect(jsonData).not.eql([]); ",
									"});",
									"",
									"pm.test(\"Response should not be 'empty array'\", function () {",
									"    pm.expect(jsonData).not.eql(undefined); ",
									"});",
									"",
									"pm.test(\"Response should not be 'empty text'\", function () {",
									"    pm.expect(jsonData).not.eql(\"\"); ",
									"});",
									"",
									"",
									"pm.test(\"Check keys in the login respose\", function () {",
									"    pm.expect(jsonData).to.be.an('object').that.has.all.keys(keysInResponse[\"firstLevel\"]);",
									"});",
									"    ",
									"pm.test(\"Check token in the response\", function () {",
									"    pm.expect(jsonData.token).not.eql(\"\"); ",
									"});",
									"",
									"pm.environment.set(\"loginToken\", jsonData.token);",
									"",
									"pm.test(\"Check keys in the login respose 'data' property\", function () {",
									"    pm.expect(jsonData.data).to.be.an('object').that.has.all.keys(keysInResponse[\"secondLevel\"]);",
									"});",
									"",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\"userName\":\"luke\",\"password\":\"admin9\"}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseURL}}/api/auth/login",
							"host": [
								"{{baseURL}}"
							],
							"path": [
								"api",
								"auth",
								"login"
							]
						}
					},
					"response": []
				}
